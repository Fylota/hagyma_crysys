          "preview": {
            "minLength": 1,
            "type": "string"
          },
          "title": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "Comment": {
        "required": [
          "content",
          "creatorName",
          "id"
        ],
        "type": "object",
        "properties": {
          "creationTime": {
            "type": "string",
            "format": "date-time",
            "nullable": true
          },
          "content": {
            "minLength": 1,
            "type": "string"
          },
          "id": {
            "minLength": 1,
            "type": "string"
          },
          "creatorName": {
